<!DOCTYPE html>
<html>
  <head>
    <title>P.O.S.</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.0/themes/base/jquery-ui.css">
   
    <script src="https://cdn.rawgit.com/harvesthq/chosen/gh-pages/chosen.jquery.min.js"></script>
    <link href="https://cdn.rawgit.com/harvesthq/chosen/gh-pages/chosen.min.css" rel="stylesheet"/>
 
  </head>

  <body>
  <div id="navbar">
    <span class="hovertext" data-hover="Click to upload Logo">
        <img src="https://eprofeng.edu.eg/files/portal/logo-placeholder.png" id="logo">
    </span>
  <a href="http://localhost:3000/<%= _("en") %>/products" class="button nav_button"> <%= _("Products") %> </a>
  <a href="http://localhost:3000/<%= _("en") %>/recipes" class="button nav_button"> <%= _("Recipes") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/clients" class="button nav_button"> <%= _("Clients") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/suppliers" class="button nav_button"> <%= _("Suppliers") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/orders" class="button nav_button"> <%= _("Orders") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/purchases" class="button nav_button"> <%= _("Purchases") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/productions" class="button nav_button"> <%= _("Production") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/inventories" class="button nav_button"> <%= _("Inventory") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/reports/inventory_report" class="button nav_button"> <%= _("Inventory Report") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/reports/product_report" class="button nav_button"> <%= _("Sales Report") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/reports/purchase_report" class="button nav_button"> <%= _("Purchasing Report") %> </a>
  <a href="http://localhost:3000/<%= _("en") %>/reports/client_report" class="button nav_button"> <%= _("Client Report") %>  </a>
  <a href="http://localhost:3000/<%= _("en") %>/uploads" class="button nav_button"> <%= _("Uploads") %>  </a>
  <div id="lang_switcher"><a href="http://localhost:3000/en/products">English</a> | <a href="http://localhost:3000/ru/products">Русски</a></div>
  </div>



  <div id="main">
      <%= yield %>
  </div>
  </body>
</html>
