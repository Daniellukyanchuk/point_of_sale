                    <li class="nav-item">
                      <a class="nav-link" href="<%= orders_path %>">
                        <i class="fa-solid fa-basket-shopping"></i> <%= _('Orders') %>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="<%= products_path %>">
                        <i class="fa-brands fa-product-hunt"></i> <%= _('Products') %>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="<%= clients_path %>">
                        <i class="fa-solid fa-user-tie"></i> <%= _('Clients') %>
                      </a>
                    </li>
