<p id="notice"><%= notice %></p>

<h1>Order Product Discounts</h1>

<table>
  <thead>
    <tr>
      <th>Discount</th>
      <th>Order product</th>
      <th>Discount quantitu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_product_discounts.each do |order_product_discount| %>
      <tr>
        <td><%= order_product_discount.discount_id %></td>
        <td><%= order_product_discount.order_product_id %></td>
        <td><%= order_product_discount.discount_quantitu %></td>
        <td><%= link_to 'Show', order_product_discount %></td>
        <td><%= link_to 'Edit', edit_order_product_discount_path(order_product_discount) %></td>
        <td><%= link_to 'Destroy', order_product_discount, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

